{
  "$schema": "https://swc.rs/schema.json",
  "minify": true,
  "jsc": {
    "target": "esnext",
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "minify": {
      "compress": true,
      "mangle": true
    }
  },
  "module": {
    "type": "nodenext",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": true
  }
}
